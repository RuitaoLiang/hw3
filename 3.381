function setup() {
  createCanvas(600,600);
colorMode(RGB, 255);
var c = color(127, 255, 0);
colorMode(RGB, 1);
var myColor = red(c);
print(myColor);
rect(0,0,300,200);
fill(color(255, 204, 0)); 
rect(0,300,300,200);
var redValue = red(c); // Get red in 'c'
print(redValue); // Print "255.0"
fill(redValue, 0, 0); // Use 'redValue' in new fill
rect(0, 200, 300, 150); 
colorMode(HSB);
fill(255, 204, 100);
rect(300, 200, 300, 150);
}
